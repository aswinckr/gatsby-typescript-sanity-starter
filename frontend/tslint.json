{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {
    "quotemark": [true, "single", "avoid-escape", "avoid-template"]
  },
  "rules": {
    "interface-name": [true, "never-prefix"],
    "member-access": [false],
    "ordered-imports": [false],
    "no-console": [false],
    "no-var-requires": false
  },
  "rulesDirectory": []
}
